# Application
NODE_ENV=development

# AI Services
SONNET_ENABLED=true
SONNET_API_KEY=your-sonnet-api-key
NOTEBOOKLM_ENABLED=false
NOTEBOOKLM_API_KEY=your-notebooklm-api-key
DEEPSEEK_ENABLED=false
DEEPSEEK_API_KEY=your-deepseek-api-key
GPT4_ENABLED=false
OPENAI_API_KEY=your-openai-api-key
PORT=3000
LOG_LEVEL=debug

# Google API Configuration
GOOGLE_CLIENT_ID=your_development_client_id
GOOGLE_CLIENT_SECRET=your_development_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# OpenAI Configuration
OPENAI_API_KEY=your_development_openai_key

# Claude Configuration
CLAUDE_API_KEY=your_development_claude_key

# Storage Configuration
GOOGLE_CLOUD_PROJECT=your_development_project_id
GOOGLE_CLOUD_BUCKET=your_development_bucket_name

# Security
JWT_SECRET=your_development_jwt_secret
ENCRYPTION_KEY=your_development_encryption_key

# Processing Configuration
CHUNK_SIZE=100000
BATCH_SIZE=50
MAX_CONCURRENT=10
MEMORY_LIMIT=0.8
QUEUE_LIMIT=1000
