{
  "name": "klein-pdf-generator",
  "version": "0.1.0",
  "description": "VSCode extension for generating PDFs from various document formats",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:klein.generatePDF"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "klein.generatePDF",
        "title": "Generate PDF"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "puppeteer": "^13.0.0"
  }
}
